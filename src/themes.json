{
    "default": "defaultDark",
    "defaultDark": "vscode",
    "defaultLight": "vscodeLight",
    "vscode": {
        "name": "Visual Studio Code",
        "dark": true,
        "fold-text": "...",
        "token-colors": {
            "heading": {
                "fontWeight": "bold"
            },
            "strong": {
                "fontWeight": "bold"
            },
            "emphasis": {
                "fontWeight": "italic"
            },
            "strikethrough": {
                "textDecoration": "line-through"
            },
            "link": {
                "color": "#6a9955",
                "textDecoration": "underline"
            },
            "comment": "#6a9955",
            "invalid": "#f44747",
            "variableName": "#9cdcfe",
            "typeName": "#569cd6",
            "propertyName": "#9cdcfe",
            "className": "#4ec9b0",
            "[constant(name), constant(variableName), constant(propertyName)]": "#4fc1ff",
            "[function_(name), function_(variableName), function_(propertyName)]": "#dcdcaa",
            "string": "#ce9178",
            "number": "#b5cea8",
            "bool": "#569cd6",
            "regexp": "#d16969",
            "escape": "#d7ba7d",
            "color": "#ce9178",
            "keyword": "#569cd6",
            "controlKeyword": "#c586c0",
            "moduleKeyword": "#c586c0",
            "self": "#569cd6",
            "null_": "#569cd6",
            "atom": "#569cd6",
            "unit": "#569cd6",
            "operator": "#d4d4d4",
            "angleBracket": "#808080",
            "squareBracket": "#d7ba7d",
            "paren": "#569cd6",
            "brace": "#b267e6",
            "documentMeta": "#6a9955",
            "inserted": "#b5cea8",
            "deleted": "#ce9178",
            "changed": "#569cd6"
        },
        "bracket-colors": [
            "#d7ba7d",
            "#b267e6",
            "#569cd6"
        ],
        "css": {
            "": {
                "font-family": "Consolas, 'Courier New', Courier, monospace",
                "font-size": "14px",
                "caret-color": "#c6c6c6",
                "background": "#1f1f1f",
                "color": "#cccccc",
                "border": "none"
            },
            "*": {
                "background": "#1f1f1f"
            },
            "a, a:visited": {
                "text-decoration": "none",
                "color": "#0078d4"
            },
            "$button$": {
                "font-family": "inherit",
                "font-size": "14px",
                "background": "#0078d4",
                "color": "#ffffff",
                "border": "1px solid #ffffff12"
            },
            "$button$hover": {
                "background": "#026ec1"
            },
            "$input$": {
                "font-size": "14px"
            },
            "$input$:not([type]), $input$[type=text]": {
                "background": "#313131",
                "color": "#cccccc",
                "border": "1px solid #3c3c3c",
                "font-family": "inherit"
            },
            "$input$[type=checkbox]": {
                "background": "#313131",
                "border": "1px solid #3c3c3c",
                "color": "#cccccc"
            },
            ":is($input$)[type=checkbox]:checked": {
                "background-color": "#202020",
                "background-image": "url(\"data:image/svg+xml;utf8,<svg xmlns='http://www.w3.org/2000/svg' version='1.1' preserveAspectRatio='none' viewBox='0 0 16 16'><line x1='2' y1='9' x2='6' y2='13' stroke='white' /><line x1='6' y1='13' x2='14' y2='3' stroke='white' /></svg>\")",
                "border": "1px solid #cccccc"
            },
            "$input$[type=number]": {
                "background": "#313131",
                "color": "#cccccc",
                "border": "1px solid #3c3c3c",
                "font-family": "inherit"
            },
            ".tab-bar, .top-bar": {
                "font-family": "Arial, Helvetica, sans-serif",
                "background": "#181818"
            },
            ".tab": {
                "font-family": "Arial, Helvetica, sans-serif",
                "font-size": "14px",
                "background": "#181818",
                "color": "#9d9d9d",
                "border": "1px solid #2b2b2b"
            },
            ".active-tab": {
                "background": "#1f1f1f",
                "color": "#ffffff",
                "border-top": "1px solid #0078d4"
            },
            ".settings": {
                "font-family": "Arial, Helvetica, sans-serif"
            },
            "$editor$": {
                "border": "1px solid #2b2b2b"
            },
            "$line-number$": {
                "color": "#6e7681"
            },
            "$current-line-number$": {
                "color": "#cccccc"
            },
            "$selection$": {
                "background": "#add6ff26"
            },
            "$selection-match$": {
                "background": "#3a3d41"
            },
            "$fold$": {
                "color": "#808080"
            },
            "$search$": {
                "font-family": "Arial, Helvetica, sans-serif",
                "font-size": "14px",
                "border-top": "1px solid #3c3c3c",
                "border-right": "1px solid #3c3c3c"
            },
            "$search-button$[name=next]:after": {
                "content": "Next"
            },
            "$search-button$[name=previous]:after": {
                "content": "Previous"
            },
            "$search-button$[name=all]:after": {
                "content": "All"
            },
            "$search-button$[name=replace]:after": {
                "content": "Replace"
            },
            "$search-button$[name=replaceAll]:after": {
                "content": "Replace All"
            }
        }
    },
    "vscodeLight": {

    }
}
